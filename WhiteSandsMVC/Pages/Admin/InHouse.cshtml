@page
@model WhiteSandsMVC.Pages.Admin.InHouseModel
@{
    ViewBag.Title = "Admin - In-House";
}
<div class="admin-heading">
    <h1 class="h1-heading">In-House Guests</h1>
</div>

<table id="DT_load" class="admin-table nowrap hover row-border order-column">
    <thead>
        <tr>
            <th>Booking ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Check-in</th>
            <th>Check-out</th>
            <th>Actions</th>
        </tr>
    </thead>
</table>


@section Scripts{
    <script type="text/javascript" src="https://cdn.jsdelivr.net/momentjs/latest/moment.min.js" asp-append-version="true"></script>
    <script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js" asp-append-version="true"></script>
    <script asp-append-version="true" src="~/js/admin/inHouse.js"></script>
}
