@page
@model WhiteSandsMVC.Pages.Admin.BookingsModel
@{
    ViewBag.Title = "Admin - Bookings";
}

<div class="admin-heading">
    <h1 class="h1-heading">Bookings</h1>
    <a class="large-btn">Create Booking</a>
</div>

<table id="DT_load" class="admin-table nowrap hover row-border order-column">
    <thead>
        <tr>
            <th>Booking ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Check-in</th>
            <th>Status</th>
            <th></th>
        </tr>
    </thead>
</table>


@section Scripts{
    <script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
    <script src="~/js/admin/bookings.js"></script>
}